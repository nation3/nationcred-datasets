{
    "singleQuote": true,
    "tabWidth": 2,
    "semi": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "importOrder": [
      "^.*/lib/.*$",
      "^.*/components/.*$",
      "^.*/pages/(?!.*.css$).*$",
      "^[./](?!.*.css$).*$",
      "^.*(css)$"
    ],
    "importOrderSeparation": false,
    "experimentalBabelParserPluginsList": ["jsx"]
  }